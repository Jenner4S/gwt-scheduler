<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:gwtscheduler.client.widgets.view.common">
	<ui:style>
		.scroll-panel {
			overflow-x: hidden;
			position: relative;
		}
		
		.container {
			width: 100%;
			height: 100%;
			overflow-x: hidden;
		}
	</ui:style>

	<g:VerticalPanel ui:field="impl" >
		<s:AdaptableWindowPanel ui:field="windowPanel">
		</s:AdaptableWindowPanel>
	</g:VerticalPanel>

</ui:UiBinder> 